extends layout

block content
    h1= title
    - let results=[{Cust_Name: "Naresh",Cust_Age:25,Mail_Id:"Naresh@gmail.com"},{Cust_Name: "suresh",Cust_Age:25,Mail_Id:"suresh@gmail.com"},{Cust_Name: "rakesh",Cust_Age:25,Mail_Id:"rakesh@gmail.com"}]
    div(class='container-fluid')
        div(class='row')
            each item in results 
                div(class='col-sm-2') 
                    p Name  is #{item.Cust_Name}
                    p Age  is #{item.Cust_Age}
                    p Mail_Id: #{item.Mail_Id}